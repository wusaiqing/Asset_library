	<url>http://maven.apache.org</url>
	<parent>
		<groupId>com.gtafe</groupId>
		<artifactId>gtafe-em</artifactId>
		<version>1.0</version>
	</parent>
	<dependencies>
		<dependency>
			<groupId>com.gtafe</groupId>
			<artifactId>gtafe-udf</artifactId>
			<version>1.0</version>
		</dependency>		
		<dependency>
			<groupId>com.gtafe</groupId>
			<artifactId>gtafe-cu-edu-trainplan</artifactId>
			<version>1.0</version>
		</dependency>
	   <dependency>
			<groupId>com.gtafe</groupId>
			<artifactId>gtafe-cu-edu-studentarchives</artifactId>
